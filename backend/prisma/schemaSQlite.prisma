datasource db {
  provider = "sqlite"
  url      = env("SQLITE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../generated/sqlite-client"
}

/* Copie os models do schema.prisma (já incluídos abaixo) */
model User {
  id        Int      @id @default(autoincrement())
  nome      String
  email     String   @unique
  senha     String
  tipo      TipoUsuario
  criadoEm  DateTime @default(now())
}

enum TipoUsuario {
  PROPRIETARIO
  CONSUMIDOR
}