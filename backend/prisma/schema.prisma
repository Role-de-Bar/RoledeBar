datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../generated/postgres-client"
}

model User {
  id        Int      @id @default(autoincrement())
  nome      String
  email     String   @unique
  senha     String
  tipo      TipoUsuario
  criadoEm  DateTime @default(now())
}

enum TipoUsuario {
  PROPRIETARIO
  CONSUMIDOR
}
